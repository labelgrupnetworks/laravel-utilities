{
    "name": "labelgrup/laravel-utilities",
    "description": "Utilities to Laravel projects",
    "type": "library",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.42|^11.0|^12.0",
        "ext-zip": "*",
        "ext-curl": "*"
    },
    "require-dev": {
        "laravel/pint": "^1.18",
        "phpunit/phpunit": "^9.5"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Labelgrup\\LaravelUtilities\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Labelgrup\\LaravelUtilities\\Test\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Eric RF",
            "email": "erojas@labelgrup.com"
        },
        {
            "name": "Manel AP",
            "email": "malonso@labelgrup.com"
        }        
    ],
    "minimum-stability": "stable",
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Labelgrup\\LaravelUtilities\\LaravelUtilitiesServiceProvider"
            ]
        }
    }
}
